#!/usr/bin/tclsh

# Comentario en tcl

# Sitio oficial: https://www.tcl-lang.org/

# Con set se asignan valores a una variable
set lenguaje Tcl;  # Comentario en línea luego de un comando terminado con ;

set entero 5;       # Número entero
set saludo Hola;    # Cadena
set verdadeeo true; # Booleano
set falso false;    # Booleano

puts "¡$saludo, $lenguaje!";    # Imprimir Texto
